{
  "name": "arcstack",
  "version": "7.0.0",
  "description": "arcstack",
  "scripts": {
    "start:server": "npm run start:dev --watch --prefix apps/server",
    "start:client": "npm run dev --prefix apps/client",
    "dev": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "publish": "git add . && git commit -m publish && git push"
