{
  "builds": [
    {
      "src": "apps/server/dist/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**"
        ]
      }
    },
    {
      "src": "apps/client/out",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/server/dist/main.js"
    }
  ]
}